{
    "name": "JK Filter",
    "version": "1.0",
    "description": "Smart content filter using machine learning. [PREDICTION_MODE]",
    "permissions": ["activeTab", "storage", "<all_urls>"],
    "content_scripts": [
        {
            "matches": [
                "http://*.twitter.com/*","https://*.twitter.com/*"],
            "js": ["src/twitter.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "icons": {
        "16": "icons/icon16px.png",
        "48": "icons/icon48px.png",
        "128": "icons/icon128px.png"
    },
    "browser_action": {
      "default_icon": "icons/icon128px.png",
      "default_popup": "popup.html"
    },
    "manifest_version": 2
}


